
extends layout

block content
	.navigateBar Navigate to:
		span.navigateOption
			a(href="/locatorDirection") Locator 
		span
			a(href="/resourcesDirection") Resources
	h1.bigBanner #{title}
	h1#resourcesBanner #{title2} 

	.newDogContainer
		form(class= "form-horizontal" role="form")
			div.form-group
				label(class= "col-sm-2 control-label" for="name") Name:
				.col-sm-10
					input(type="name" class="form-control")
			div.form-group
				label(class= "col-sm-2 control-label" for="name") Age:
				.col-sm-10
					input(type="name" class="form-control")
			div.form-group
				label(class= "col-sm-2 control-label" for="name") Breed:
				.col-sm-10
					input(type="name" class="form-control")
			div.form-group
				label(class= "col-sm-2 control-label" for="name") Sex:
				.col-sm-10
					input(type="name" class="form-control")
			div.form-group
				label(class= "col-sm-2 control-label" for="name") Foster:
				.col-sm-10
					input(type="name" class="form-control")
			div.form-group
				label(class= "col-sm-2 control-label" for="name") Biography:
				.col-sm-10
					textarea(type="name" class="form-control")
			div.form-group
				label(class= "col-sm-2 control-label" for="name") Image(URL):
				.col-sm-10
					input(type="name" class="form-control")
				.form-group
					.col-sm-offset-2.col-sm-10
						button(type="submit" class="btn btn-default") Submit

			